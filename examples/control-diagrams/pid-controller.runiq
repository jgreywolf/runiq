diagram "PID Controller" {

  shape refs as @rect label:"Reference"
  shape sum as @junction label:"+/-"
  shape pids as @transferFunction label:"PID"
  shape plant as @transferFunction label:"Plant\nG(s)"
  shape output as @rect label:"Output"

  refs -r_t-> sum
  sum -e_t-> pids
  pids -u_t-> plant
  plant -y_t-> output
  output -feedback-> sum

}
