diagram "PID Controller" {

  shape ref as @rect label:"Reference"
  shape sum as @junction label:"+/-"
  shape pid as @transferFn label:"PID"
  shape plant as @transferFn label:"Plant\nG(s)"
  shape output as @rect label:"Output"

  ref -r_t-> sum
  sum -e_t-> pid
  pid -u_t-> plant
  plant -y_t-> output
  output -feedback-> sum

}
