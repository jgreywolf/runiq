diagram "Cascaded Transfer Functions" {

  shape input as @rect label:"Input"
  shape g1 as @transferFunction label:"K/(s+1)"
  shape g2 as @transferFunction label:"10/(s+5)"
  shape g3 as @integrator label:"1/s"
  shape output as @rect label:"Output"

  input -> g1
  g1 -> g2
  g2 -> g3
  g3 -> output

}
