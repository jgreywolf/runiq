diagram "Simple Flowchart Test" {
  
  shape start as @rounded label:"Start"
  shape input as @lean-r label:"Get Input"
  shape process as @rect label:"Process Data"
  shape decision as @rhombus label:"Valid?"
  shape output as @doc label:"Generate Report"
  shape end as @rounded label:"End"
  
  start -> input
  input -> process
  process -> decision
  decision -yes-> output
  decision -no-> input
  output -> end
}
