
hydraulic "Test New Pumps and Motors" {
  // Test circuit demonstrating all new pump and motor types
  // Each pump connects to its own motor to show both component types

  net TANK, PRESSURE1, PRESSURE2, PRESSURE3, PRESSURE4, PRESSURE5, RETURN

  pressure 150 bar operating
  flowRate 30 L/min
  fluid mineral "ISO VG 46" temp:(20, 60, degC)

  // Reservoir for the system
  part RESERVOIR type:RESERVOIR pins:(TANK,RETURN) doc:"Main reservoir"

  // ===== PUMP AND MOTOR PAIRS =====
  // Circuit 1: Gear pump -> Gear motor
  part P1 type:PUMP_GEAR pins:(TANK,PRESSURE1) doc:"Gear pump"
  part M1 type:MOTOR_GEAR pins:(PRESSURE1,RETURN) doc:"Gear motor"

  // Circuit 2: Vane pump -> Vane motor
  part P2 type:PUMP_VANE pins:(TANK,PRESSURE2) doc:"Vane pump"
  part M2 type:MOTOR_VANE pins:(PRESSURE2,RETURN) doc:"Vane motor"

  // Circuit 3: Axial piston pump -> Axial piston motor
  part P3 type:PUMP_PISTON_AXIAL pins:(TANK,PRESSURE3) doc:"Axial piston pump"
  part M3 type:MOTOR_PISTON_AXIAL pins:(PRESSURE3,RETURN) doc:"Axial piston motor"

  // Circuit 4: Radial piston pump -> Radial piston motor
  part P4 type:PUMP_PISTON_RADIAL pins:(TANK,PRESSURE4) doc:"Radial piston pump"
  part M4 type:MOTOR_PISTON_RADIAL pins:(PRESSURE4,RETURN) doc:"Radial piston motor"

  // Circuit 5: Screw pump -> Orbit motor
  part P5 type:PUMP_SCREW pins:(TANK,PRESSURE5) doc:"Screw pump"
  part M5 type:MOTOR_ORBIT pins:(PRESSURE5,RETURN) doc:"Orbit motor"

  // Additional pump types (standalone to show variety)
  part P6 type:PUMP_HAND pins:(TANK,RETURN) doc:"Hand pump"
  part P7 type:PUMP_PISTON_VAR pins:(TANK,RETURN) doc:"Variable piston pump"
}
