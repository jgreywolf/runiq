
hydraulic "Phase 1 Hydraulic Components Test" {
  // Test file demonstrating all 28 new Phase 1 components
  // 7 pumps + 5 motors + 9 valves + 7 cylinders

  net TANK, SUPPLY, PRESSURE, RETURN, CYL_A, CYL_B

  pressure 200 bar operating
  flowRate 50 L/min
  fluid mineral "ISO VG 46" temp:(20, 60, degC)

  // Reservoir
  part RES type:RESERVOIR pins:(TANK,RETURN) doc:"Main hydraulic reservoir"

  // ===== SECTION 1: PUMPS (7 types) =====
  part P1 type:PUMP_GEAR pins:(TANK,PRESSURE) doc:"Gear pump - fixed displacement"
  part P2 type:PUMP_VANE pins:(TANK,PRESSURE) doc:"Vane pump - good efficiency"
  part P3 type:PUMP_PISTON_AXIAL pins:(TANK,PRESSURE) doc:"Axial piston pump - high pressure"
  part P4 type:PUMP_PISTON_RADIAL pins:(TANK,PRESSURE) doc:"Radial piston pump - very high pressure"
  part P5 type:PUMP_SCREW pins:(TANK,PRESSURE) doc:"Screw pump - smooth flow"
  part P6 type:PUMP_HAND pins:(TANK,PRESSURE) doc:"Hand pump - manual operation"
  part P7 type:PUMP_PISTON_VAR pins:(TANK,PRESSURE) doc:"Variable piston pump - adjustable flow"

  // ===== SECTION 2: MOTORS (5 types) =====
  part M1 type:MOTOR_GEAR pins:(PRESSURE,RETURN) doc:"Gear motor - fixed displacement"
  part M2 type:MOTOR_VANE pins:(PRESSURE,RETURN) doc:"Vane motor - good starting torque"
  part M3 type:MOTOR_PISTON_AXIAL pins:(PRESSURE,RETURN) doc:"Axial piston motor - high efficiency"
  part M4 type:MOTOR_PISTON_RADIAL pins:(PRESSURE,RETURN) doc:"Radial piston motor - high torque"
  part M5 type:MOTOR_ORBIT pins:(PRESSURE,RETURN) doc:"Orbit motor - gerotor type"

  // ===== SECTION 3: DIRECTIONAL CONTROL VALVES (9 types) =====
  part V1 type:VALVE_22 pins:(SUPPLY,CYL_A) doc:"2/2-way valve - on/off control"
  part V2 type:VALVE_32_HYD pins:(SUPPLY,CYL_A,RETURN) doc:"3/2-way valve - 3 port control"
  part V3 type:VALVE_42 pins:(SUPPLY,CYL_A,CYL_B,RETURN) doc:"4/2-way valve - reversing"
  part V4 type:VALVE_43 pins:(SUPPLY,CYL_A,CYL_B,RETURN) doc:"4/3-way valve - standard center"
  part V5 type:VALVE_43_CLOSED pins:(SUPPLY,CYL_A,CYL_B,RETURN) doc:"4/3 closed center - all ports blocked"
  part V6 type:VALVE_43_OPEN pins:(SUPPLY,CYL_A,CYL_B,RETURN) doc:"4/3 open center - P to T"
  part V7 type:VALVE_43_TANDEM pins:(SUPPLY,CYL_A,CYL_B,RETURN) doc:"4/3 tandem center - P to T, A/B blocked"
  part V8 type:VALVE_43_FLOAT pins:(SUPPLY,CYL_A,CYL_B,RETURN) doc:"4/3 float center - A/B to T, P blocked"
  part V9 type:VALVE_PROPORTIONAL pins:(SUPPLY,CYL_A,CYL_B,RETURN,TANK) doc:"Proportional valve - variable flow"
  part V10 type:VALVE_SERVO pins:(SUPPLY,CYL_A,CYL_B,RETURN,TANK) doc:"Servo valve - precision control"

  // ===== SECTION 4: CYLINDERS (7 types) =====
  part C1 type:CYL_SINGLE_ROD pins:(TANK,CYL_A,CYL_B) doc:"Single rod cylinder - differential area"
  part C2 type:CYL_DOUBLE_ROD pins:(TANK,TANK,CYL_A,CYL_B) doc:"Double rod cylinder - equal area"
  part C3 type:CYL_TELESCOPIC pins:(TANK,CYL_A,CYL_B) doc:"Telescopic cylinder - long stroke"
  part C4 type:CYL_MILL pins:(TANK,CYL_A,CYL_B) doc:"Mill cylinder - heavy duty"
  part C5 type:CYL_TIE_ROD pins:(TANK,CYL_A,CYL_B) doc:"Tie-rod cylinder - economical"
  part C6 type:CYL_WELDED pins:(TANK,CYL_A,CYL_B) doc:"Welded cylinder - mobile equipment"
  part C7 type:CYL_FEEDBACK pins:(TANK,CYL_A,CYL_B,RETURN) doc:"Position feedback cylinder - sensor integrated"
}
