
hydraulic "Phase 3 Hydraulic Support Equipment Test" {
  // Test file demonstrating all 14 Phase 3 components
  // 6 filters + 4 heat exchangers + 4 manifolds
  // NOTE: Components are connected by sharing net names in pins:()
  // When two parts use the same net name, they are automatically connected

  net TANK, SUCTION, PUMP, PRESSURE, WORK, RETURN, CLEAN, COOL
  net HOT_IN, HOT_OUT, COLD_IN, COLD_OUT
  net MANIFOLD_P, MANIFOLD_A, MANIFOLD_B, MANIFOLD_T
  net PORT1, PORT2, PORT3

  pressure 200 bar operating
  flowRate 60 L/min
  fluid mineral "ISO VG 46" temp:(25, 55, degC)

  // Reservoir - connects to TANK and RETURN nets
  part RES type:RESERVOIR pins:(TANK,RETURN) doc:"Main hydraulic reservoir"

  // ===== SECTION 1: FILTERS (6 types) =====
  // Suction filter connects SUCTION net to PUMP net
  part FS1 type:FILTER_SUCTION pins:(SUCTION,PUMP) doc:"Suction filter - coarse filtration before pump"

  // Pressure filter connects PUMP net to PRESSURE net
  part FP1 type:FILTER_PRESSURE pins:(PUMP,PRESSURE) doc:"Pressure filter - fine filtration after pump"

  // Return filter connects RETURN net back to TANK net
  part FR1 type:FILTER_RETURN pins:(RETURN,TANK) doc:"Return line filter - before tank"

  // Offline filter provides continuous cleaning between CLEAN and TANK nets
  part FO1 type:FILTER_OFFLINE pins:(CLEAN,TANK) doc:"Offline kidney-loop filter - continuous cleaning"

  // Breather filter connects to TANK net only
  part FB1 type:FILTER_BREATHER pins:(TANK) doc:"Breather filter - prevents contamination from atmosphere"

  // Spin-on filter connects PRESSURE to WORK net
  part FSO1 type:FILTER_SPIN_ON pins:(PRESSURE,WORK) doc:"Spin-on cartridge filter - easy replacement"

  // ===== SECTION 2: HEAT EXCHANGERS / COOLERS (4 types) =====
  // Air cooler connects WORK to COOL net
  part CA1 type:COOLER_AIR pins:(WORK,COOL) doc:"Air cooler - ambient air cooling"

  // Water cooler with separate oil and water circuits
  part CW1 type:COOLER_WATER pins:(HOT_IN,HOT_OUT,COLD_IN,COLD_OUT) doc:"Water cooler - shell and tube design"

  // Oil-to-air compact cooler
  part COA1 type:COOLER_OIL_AIR pins:(HOT_IN,HOT_OUT) doc:"Oil-to-air compact cooler"

  // Oil-to-water shell and tube cooler connects COOL to RETURN
  part COW1 type:COOLER_OIL_WATER pins:(COOL,RETURN,COLD_IN,COLD_OUT) doc:"Oil-to-water shell and tube cooler"

  // ===== SECTION 3: MANIFOLDS (4 types) =====
  // Sandwich manifold distributes flow
  part MS1 type:MANIFOLD_SANDWICH pins:(MANIFOLD_P,MANIFOLD_T,MANIFOLD_A,MANIFOLD_B) doc:"Sandwich plate manifold - between components"

  // Monoblock manifold with standard P, A, B, T ports
  part MM1 type:MANIFOLD_MONOBLOCK pins:(MANIFOLD_P,MANIFOLD_A,MANIFOLD_B,MANIFOLD_T) doc:"Monoblock manifold - single machined block"

  // Modular manifold with 3 connection ports
  part MD1 type:MANIFOLD_MODULAR pins:(PORT1,PORT2,PORT3) doc:"Modular stackable manifold sections"

  // Cartridge manifold with cavity for cartridge valve
  part MC1 type:MANIFOLD_CARTRIDGE pins:(PORT1,PORT2,PORT3,MANIFOLD_A) doc:"Cartridge valve manifold cavity"
}
