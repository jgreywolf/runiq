diagram "Edge Routing Test" {
  direction TB
  
  // Simple vertical layout
  shape n1 as @rectangle label:"Node 1"
  shape n2 as @rectangle label:"Node 2"
  shape n3 as @rectangle label:"Node 3"
  shape n4 as @rectangle label:"Node 4"
  shape n5 as @rectangle label:"Node 5"
  
  // Create edges that might route diagonally
  n1 -> n2
  n1 -> n3
  n2 -> n4
  n3 -> n4
  n4 -> n5
  n1 -> n5
}
