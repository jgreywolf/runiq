
hydraulic "Phase 2 Hydraulic Components Test" {
  // Test file demonstrating all 23 new Phase 2 components
  // 6 pressure control + 6 flow control + 3 check valves + 4 rotary actuators + 4 accumulators

  net TANK, PUMP, PRESSURE, WORK, RETURN, LOAD, PILOT, SENSE
  net MOTOR_A, MOTOR_B, SHAFT, EXCESS, PRIORITY

  pressure 250 bar operating
  flowRate 80 L/min
  fluid mineral "ISO VG 46" temp:(20, 60, degC)

  // Reservoir
  part RES type:RESERVOIR pins:(TANK,RETURN) doc:"Main hydraulic reservoir"

  // ===== SECTION 1: PRESSURE CONTROL VALVES (6 types) =====
  part RV1 type:RELIEF_DIRECT pins:(PRESSURE,TANK) doc:"Direct-acting relief valve - simple design"
  part RV2 type:RELIEF_PILOT pins:(PRESSURE,TANK) doc:"Pilot-operated relief - high flow capacity"
  part UV1 type:UNLOADING_VALVE pins:(PUMP,TANK,PILOT) doc:"Unloading valve - vents pump at low pressure"
  part SV1 type:SEQUENCE_VALVE pins:(PRESSURE,WORK,SENSE) doc:"Sequence valve - enables secondary circuit"
  part CB1 type:COUNTERBALANCE_VALVE pins:(LOAD,WORK,PILOT) doc:"Counterbalance valve - holds vertical loads"
  part BV1 type:BRAKE_VALVE pins:(MOTOR_A,WORK,MOTOR_B) doc:"Brake valve - prevents motor overspeed"

  // ===== SECTION 2: FLOW CONTROL VALVES (6 types) =====
  part TV1 type:THROTTLE_VALVE pins:(PRESSURE,WORK) doc:"Throttle valve - fixed orifice"
  part NV1 type:NEEDLE_VALVE pins:(PRESSURE,WORK) doc:"Needle valve - fine adjustment"
  part FC1 type:FLOW_COMPENSATED pins:(PRESSURE,WORK) doc:"Pressure compensated flow control"
  part FT1 type:FLOW_TEMP_COMP pins:(PRESSURE,WORK) doc:"Temperature compensated flow control"
  part PV1 type:PRIORITY_VALVE pins:(PUMP,PRIORITY,EXCESS) doc:"Priority valve - critical circuit first"
  part FD1 type:FLOW_DIVIDER pins:(PUMP,WORK,RETURN) doc:"Flow divider - equal flow split"

  // ===== SECTION 3: CHECK VALVES (3 types) =====
  part PC1 type:CHECK_PILOT pins:(PRESSURE,WORK,PILOT) doc:"Pilot-operated check - locks flow"
  part SH1 type:SHUTTLE_VALVE pins:(PRESSURE,WORK,RETURN) doc:"Shuttle valve - OR function"
  part PO1 type:CHECK_PILOT_OPEN pins:(PRESSURE,WORK,PILOT) doc:"Pilot to open check - reverse flow with pilot"

  // ===== SECTION 4: ROTARY ACTUATORS (4 types) =====
  part RA1 type:ROTARY_VANE pins:(PRESSURE,RETURN,SHAFT) doc:"Vane rotary actuator - limited rotation"
  part RA2 type:ROTARY_PISTON pins:(PRESSURE,RETURN,SHAFT) doc:"Piston rotary actuator - high torque"
  part RP1 type:RACK_PINION pins:(PRESSURE,RETURN,SHAFT) doc:"Rack and pinion - linear to rotary"
  part HA1 type:HELICAL_ACTUATOR pins:(PRESSURE,SHAFT) doc:"Helical actuator - smooth rotation"

  // ===== SECTION 5: ACCUMULATORS (4 types) =====
  part AB1 type:ACCUMULATOR_BLADDER pins:(PRESSURE) doc:"Bladder accumulator - fast response"
  part AP1 type:ACCUMULATOR_PISTON pins:(PRESSURE) doc:"Piston accumulator - large volume"
  part AD1 type:ACCUMULATOR_DIAPHRAGM pins:(PRESSURE) doc:"Diaphragm accumulator - small volume"
  part AW1 type:ACCUMULATOR_WEIGHT pins:(PRESSURE) doc:"Weight-loaded accumulator - constant pressure"
}
