{
  "name": "@runiq/data-loader",
  "version": "0.1.0",
  "description": "Data source loaders for JSON and CSV files",
  "type": "module",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsup",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:coverage": "vitest run --coverage"
  },
  "keywords": [
    "data-loader",
    "json",
    "csv",
    "validation"
  ],
  "author": "Runiq Contributors",
  "license": "MIT",
  "devDependencies": {
    "@types/node": "^20.11.5",
    "@types/papaparse": "^5.3.14",
    "tsup": "^8.0.1",
    "typescript": "^5.9.0",
    "vitest": "^1.2.0"
  },
  "dependencies": {
    "papaparse": "^5.4.1"
  }
}
