{
  "name": "@runiq/cli",
  "version": "0.1.0",
  "description": "Command line interface for Runiq",
  "type": "module",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "bin": {
    "runiq": "./dist/cli.js"
  },
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js"
    }
  },
  "files": [
    "dist",
    "src"
  ],
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch",
    "test": "vitest run",
    "test:watch": "vitest",
    "lint": "eslint src/**/*.ts",
    "typecheck": "tsc --noEmit",
    "clean": "rm -rf dist"
  },
  "dependencies": {
    "@runiq/core": "workspace:*",
    "@runiq/parser-dsl": "workspace:*",
    "@runiq/io-json": "workspace:*",
    "@runiq/layout-base": "workspace:*",
    "@runiq/renderer-svg": "workspace:*",
    "@runiq/icons-fontawesome": "workspace:*",
    "commander": "^11.1.0"
  },
  "devDependencies": {
    "@types/node": "^20.11.0",
    "tsup": "^8.0.2",
    "typescript": "^5.3.3",
    "vitest": "^1.2.2"
  },
  "keywords": [
    "cli",
    "diagram",
    "svg"
  ],
  "author": "Runiq Team",
  "license": "MIT"
}
