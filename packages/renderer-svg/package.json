{
  "name": "@runiq/renderer-svg",
  "version": "0.1.0",
  "description": "SVG renderer for Runiq diagrams",
  "type": "module",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js"
    }
  },
  "files": [
    "dist",
    "src"
  ],
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:visual": "playwright test",
    "test:visual:update": "playwright test --update-snapshots",
    "test:visual:ui": "playwright test --ui",
    "test:visual:report": "playwright show-report",
    "test:visual:serve": "node tests/visual/server.mjs",
    "lint": "eslint src/**/*.ts",
    "typecheck": "tsc --noEmit",
    "clean": "rm -rf dist"
  },
  "peerDependencies": {
    "@runiq/core": "workspace:*"
  },
  "dependencies": {
    "@runiq/parser-dsl": "workspace:*"
  },
  "devDependencies": {
    "@playwright/test": "^1.55.1",
    "@runiq/core": "workspace:*",
    "@runiq/icons-fontawesome": "workspace:^",
    "@runiq/layout-base": "workspace:^",
    "@types/node": "^20.11.0",
    "playwright": "^1.55.1",
    "tsup": "^8.0.2",
    "typescript": "^5.3.3",
    "vitest": "^1.2.2"
  },
  "keywords": [
    "svg",
    "renderer",
    "diagram"
  ],
  "author": "Runiq Team",
  "license": "MIT"
}
