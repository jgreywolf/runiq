{
  "name": "@runiq/export-verilog",
  "version": "0.1.0",
  "description": "Verilog exporter for Runiq digital circuits",
  "type": "module",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js"
    }
