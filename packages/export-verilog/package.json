  "keywords": [
    "verilog",
    "hdl",
    "digital",
    "circuit",
    "export"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "@runiq/core": "workspace:*"
  },
  "devDependencies": {
    "@types/node": "^20.11.5",
    "tsup": "^8.0.1",
    "typescript": "^5.3.3",
    "vitest": "^1.1.3"
  }
}
