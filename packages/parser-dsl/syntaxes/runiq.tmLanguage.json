sible:|collapseIcon:|collapseKeyboardShortcut:|collapseMode:|collapsePersistState:|collapseRedirectEdges:|collapseShowCount:|collapseStateKey:|collapseSummary:|collapseTransitionState:|collapsed:|collapsible:|color:|colors:|constraint:|constraints:|crossContainerEdgeOptimization:|data:|deceased:|default:|depth:|depthIndicatorStyle:|derived:|description:|direction:|distribution:|doActivity:|edgeBundling:|edgeRouting:|edgeType:|effect:|entry:|event:|evolution:|exit:|extends:|extensionPoints:|fill:|flowType:|fontFamily:|fontSize:|fontWeight:|from:|gates:|gatewayType:|genericTypes:|guard:|header:|headerBackgroundColor:|headerPosition:|hoverBorderColor:|hoverBorderWidth:|hoverHighlight:|icon:|iconColor:|iconSize:|incrementalLayout:|inertia:|inputPins:|label:|labelPosition:|layoutCache:|layoutComplexity:|legendPosition:|lineStyle:|link:|map:|margin:|marginBottom:|marginLeft:|marginRight:|marginTop:|maxHeight:|maxWidth:|methods:|minHeight:|minResizeHeight:|minResizeWidth:|minWidth:|multiplicitySource:|multiplicityTarget:|name:|navigability:|nodeSpacing:|of:|opacity:|orientation:|outputPins:|padding:|paddingBottom:|paddingLeft:|paddingRight:|paddingTop:|parameters:|params:|participants:|pins:|ports:|position:|preset:|ref:|relationship:|resizable:|resizeHandles:|returnType:|roleSource:|roleTarget:|routing:|selectionBorderColor:|selectionBorderWidth:|selectionHighlight:|shadow:|showChildCount:|showDepthIndicator:|showLegend:|source:|spacing:|stacked:|stateInvariant:|static:|stereotype:|stereotypes:|stroke:|strokeWidth:|style:|temp:|templateId:|textAlign:|timing:|title:|to:|tooltip:|type:|value:|verticalAlign:|visibility:|xLabel:|yLabel:)\\B"
    },
    {
      "name": "string.quoted.double.runiq",
      "begin": "\"",
      "end": "\"",
      "patterns": [
        {
          "include": "#string-character-escape"
        }
      ]
    }
  ],
  "repository": {
    "comments": {
      "patterns": [
        {
          "name": "comment.block.runiq",
          "begin": "/\\*",
          "beginCaptures": {
            "0": {
              "name": "punctuation.definition.comment.runiq"
            }
          },
          "end": "\\*/",
          "endCaptures": {
            "0": {
              "name": "punctuation.definition.comment.runiq"
            }
          }
        },
        {
          "begin": "//#",
          "beginCaptures": {
            "1": {
              "name": "punctuation.whitespace.comment.leading.runiq"
            }
          },
          "end": "(?=$)",
          "name": "comment.line.runiq"
        }
      ]
    },
    "string-character-escape": {
      "name": "constant.character.escape.runiq",
      "match": "\\\\(x[0-9A-Fa-f]{2}|u[0-9A-Fa-f]{4}|u\\{[0-9A-Fa-f]+\\}|[0-2][0-7]{0,2}|3[0-6][0-7]?|37[0-7]?|[4-7][0-7]?|.|$)"
    }
  }
