{
  "name": "runiq",
  "scopeName": "source.runiq",
  "fileTypes": [
    ".runiq"
  ],
  "patterns": [
    {
      "include": "#comments"
    },
    {
      "name": "keyword.control.runiq",
      "match": "\\b(BT|CFM|GPM|K|L/min|L/s|LR|MPa|RL|TB|ac|actor|aggregation|alt|analysis|anchor|as|association|async|bar|bidirectional|biodegradable|bottom|boundary|break|center|collapsing|component|composition|container|container-aware|control|create|critical|dashed|database|dc|degC|degF|dependency|destroy|diagram|digital|direction|dotted|double|ease-in|ease-in-out|ease-out|electrical|entity|evolve|expanding|false|fill-available|fit-content|flowRate|fluid|force|found|fragment|full|generalization|group|high|hollow|hydraulic|inst|kPa|layered|left|linear|loop|lost|low|max|medium|message|middle|min|mindmap|mineral|module|mrtree|mÂ³/h|net|noise|none|note|op|open|operating|opt|orthogonal|over|package|packed|par|part|partial|participant|phosphate-ester|pneumatic|polyline|pressure|private|protected|psi|public|radial|rated|realization|return|right|routing|schematic|sequence|shape|solid|source|space-around|space-between|space-evenly|spline|splines|stable|standard|straight|stress|style|sync|synthetic|target|to|top|tran|true|wardley|water-glycol)\\b|\\b(abstract:|activate:|affected:|algorithm:|alignContent:|alternatives:|arrowType:|attributes:|autoResize:|backgroundColor:|badge:|borderColor:|borderStyle:|borderWidth:|carrier:|collapseAnimationDuration:|collapseAnimationEasing:|collapseIcon:|collapseKeyboardShortcut:|collapseMode:|collapsePersistState:|collapseRedirectEdges:|collapseShowCount:|collapseStateKey:|collapseSummary:|collapseTransitionState:|collapsed:|collapsible:|colors:|constraints:|crossContainerEdgeOptimization:|data:|deceased:|default:|depth:|derived:|distribution:|edgeBundling:|edgeRouting:|edgeType:|evolution:|from:|genericTypes:|guard:|header:|headerBackgroundColor:|headerPosition:|icon:|iconColor:|iconSize:|incrementalLayout:|inertia:|label:|labelPosition:|layoutCache:|layoutComplexity:|legendPosition:|lineStyle:|link:|map:|margin:|marginBottom:|marginLeft:|marginRight:|marginTop:|maxHeight:|maxWidth:|methods:|minHeight:|minWidth:|multiplicitySource:|multiplicityTarget:|name:|navigability:|nodeSpacing:|of:|opacity:|padding:|paddingBottom:|paddingLeft:|paddingRight:|paddingTop:|params:|participants:|pins:|ports:|position:|returnType:|roleSource:|roleTarget:|routing:|shadow:|showLegend:|source:|spacing:|stacked:|static:|stereotype:|style:|temp:|timing:|title:|to:|tooltip:|type:|value:|verticalAlign:|visibility:|xLabel:|yLabel:)\\B"
    },
    {
      "name": "string.quoted.double.runiq",
      "begin": "\"",
      "end": "\"",
      "patterns": [
        {
          "include": "#string-character-escape"
        }
      ]
    }
  ],
  "repository": {
    "comments": {
      "patterns": [
        {
          "name": "comment.block.runiq",
          "begin": "/\\*",
          "beginCaptures": {
            "0": {
              "name": "punctuation.definition.comment.runiq"
            }
          },
          "end": "\\*/",
          "endCaptures": {
            "0": {
              "name": "punctuation.definition.comment.runiq"
            }
          }
        },
        {
          "begin": "//#",
          "beginCaptures": {
            "1": {
              "name": "punctuation.whitespace.comment.leading.runiq"
            }
          },
          "end": "(?=$)",
          "name": "comment.line.runiq"
        }
      ]
    },
    "string-character-escape": {
      "name": "constant.character.escape.runiq",
      "match": "\\\\(x[0-9A-Fa-f]{2}|u[0-9A-Fa-f]{4}|u\\{[0-9A-Fa-f]+\\}|[0-2][0-7]{0,2}|3[0-6][0-7]?|37[0-7]?|[4-7][0-7]?|.|$)"
    }
  }
}
