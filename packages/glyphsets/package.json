{
  "name": "@runiq/glyphsets",
  "version": "0.1.0",
  "description": "Pre-built diagram patterns (GlyphSets) for Runiq - SmartArt-style templates",
  "type": "module",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "types": "./dist/index.d.ts"
    }
  },
  "scripts": {
    "build": "tsup && tsc -p tsconfig.build.json",
    "dev": "tsup --watch",
    "test": "vitest run",
    "test:watch": "vitest",
    "typecheck": "tsc --noEmit",
    "clean": "rimraf dist"
  },
  "keywords": [
    "runiq",
    "diagram",
    "glyphset",
    "template",
    "smartart",
    "visualization"
  ],
  "author": "Runiq",
  "license": "MIT",
  "dependencies": {
    "@runiq/core": "workspace:*"
  },
  "devDependencies": {
    "@types/node": "^20.11.5",
    "typescript": "^5.3.3",
    "vitest": "^1.2.0",
    "rimraf": "^5.0.5",
    "tsup": "^8.0.2"
  }
}
