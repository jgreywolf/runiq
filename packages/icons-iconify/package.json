{
  "name": "@runiq/icons-iconify",
  "version": "0.1.0",
  "description": "Iconify-style icon provider for Runiq",
  "type": "module",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js"
    }
  },
  "files": [
    "dist",
    "src"
  ],
  "scripts": {
    "build": "tsup && tsc -p tsconfig.build.json",
    "dev": "tsup --watch",
    "extract:iconify": "node ./scripts/extract-iconify.mjs",
    "test": "vitest run",
    "test:watch": "vitest",
    "lint": "eslint src/**/*.ts",
    "typecheck": "tsc --noEmit",
    "clean": "rm -rf dist"
  },
  "dependencies": {
    "@runiq/core": "workspace:*"
  },
  "devDependencies": {
    "@iconify-json/logos": "^1.2.10",
    "@iconify-json/simple-icons": "^1.2.68",
    "@types/node": "^20.11.0",
    "tsup": "^8.0.2",
    "typescript": "^5.3.3",
    "vitest": "^1.2.2"
  },
  "keywords": [
    "icons",
    "iconify",
    "svg"
  ],
  "author": "Runiq Team",
  "license": "MIT"
}
