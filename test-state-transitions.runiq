// Test state machine transition labels
diagram "State Machine Transition Test" {
  shape idle as @state label:"Idle"
  shape processing as @state label:"Processing"
  shape complete as @state label:"Complete"
  shape error as @state label:"Error"

  // Transition with no properties (should have no label)
  idle -> processing

  // Transition with event only
  processing -> complete event:"done"

  // Transition with guard only
  processing -> error guard:"[failed]"

  // Transition with effect only
  error -> idle effect:"/ reset()"

  // Transition with event and guard
  idle -> complete event:"skip" guard:"[fastTrack]"

  // Transition with all three: event [guard] / effect
  complete -> idle event:"restart" guard:"[allowed]" effect:"/ cleanup()"
}
